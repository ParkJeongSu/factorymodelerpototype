{
  "name": "factorymodelerpototype",
  "version": "1.0.0",
  "description": "factorymodelerpototype",
  "main": "./src/electron.js",
  "keywords": [
    "electron",
    "factoryModeler"
  ],
  "author": "ParkJeongSu",
  "license": "MIT",
  "scripts": {
